node {
   stage('Get GitHub'){
      git 'https://github.com/ydcqy/cloud-test.git'
   }
   mvnHome = tool 'maven-3.5.2'
   stage('Maven Build'){
      sh "${mvnHome}/bin/mvn -f personal-web/pom.xml clean package"
   }

   stage('Docker Build'){
      sh "scp -r ${WORKSPACE}/personal-web/target/*.jar root@g1.ydcqy.com:/opt/docker/build_dir"
      echo '${WORKSPACE}'
      echo "${WORKSPACE}"
   }
}